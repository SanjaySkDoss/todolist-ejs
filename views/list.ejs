<%- include("header") %>

<body>

    <div class="box" id="heading">
        <h1><%=listName %></h1>
    </div>

    <div class="box">
        <%      for(let i=0;i<array.length;i++){ %>
        <div class="item ">
            <input type="checkbox">
            <p > <%= array[i] %></p>
        </div>
            <%     } %>
      
        <form class="item" action="/" method="post">
            <input  type="text" name="itemname" placeholder="new item">

            <button type="submit" name="button" value=<%= listName %> >+</button>
        </form>

    </div>
   

<%- include("footer") %>

</body>

</html>